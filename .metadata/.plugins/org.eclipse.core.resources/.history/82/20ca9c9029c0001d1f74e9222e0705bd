import PhanTrang.Dto.ProductsDtoMapper;

public class ProductsDao extends BaseDao {
	public List<ProductsDto> GetDataProducts() {
		List<ProductsDto> listProducts = _jdbcTemplate.query(sql, new ProductsDtoMapper())
				return listProducts
